{
  "manifest_version": 2,
  "name": "Astro Wallet",
  "version": "1.0",
  "description": "Future of CRYPTO Wallet",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}